  "main": "bin/app.js",
  "scripts": {
    "build": "babel -w code/ -d build -s",
    "start-prod-api": "better-npm-run start-prod-api",
    "dev-api": "better-npm-run start-dev-api"
  },
  "betterScripts": {
    "start-prod-api": {
      "command": "node ./bin/api",
      "env": {
        "NODE_PATH": "./api",
        "NODE_ENV": "production",
        "APIPORT": 6969
      }
    },
    "start-dev-api": {
      "command": "node ./bin/api",
      "env": {
        "NODE_PATH": "./api",
        "NODE_ENV": "development",
        "APIPORT": 3030
      }
    }
  },
